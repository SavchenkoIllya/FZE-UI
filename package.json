{
  "name": "@SavchenkoIllya/fze-ui",
  "version": "0.1.3",
  "description": "UI components for FZE",
  "author": "SavchenkoIllya",
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist", "index.js"],
  "sideEffects": false,
  "scripts": {
    "build:all": "npm run build & npm run build:browser & npm run typescript-declarations",
    "watch:all": "npm run watch & npm run watch:browser",
    "build": "node build/main.js",
    "watch": "node build/main.js -- watch",
    "build:browser": "node build/browser.js",
    "watch:browser": "node build/browser.js -- watch",
    "eslint": "eslint ./src/**",
    "typescript-declarations": "tsc --emitDeclarationOnly"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SavchenkoIllya/FZE-UI.git"
  },
  "peerDependencies": {
    "next": ">=14.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.2.0"
  },
  "devDependencies": {
    "esbuild": "^0.19.9",
    "esbuild-plugin-copy": "^2.1.1",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-typescript": "^7.23.3",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "^15.3.0",
    "next": "^15.3.0",
    "prettier": "3.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rimraf": "^5.0.5",
    "tailwindcss": "^4",
    "tsx": "^4.19.3",
    "typescript": "^5"
  }
